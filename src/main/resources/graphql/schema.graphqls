type Query {
    messages: [Message]
}

type Message {
    id: String!
    text: String!
}

type Profile {
    id: String!
    firstName: String!
    middleName: String
    lastName: String!
    gender: Int!
    phone: String
    email: String
    photo: String
}

input ProfileInput {
    firstName: String!
    middleName: String
    lastName: String!
    gender: Int!
    phone: String
    email: String
    photo: String
}
